           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"frictionType", "\[Equal]", " ", "\"\<Kinetic\>\""}], 
             ",", 
             RowBox[{"FfK", " ", "FfHat"}], ",", 
             RowBox[{"-", "Fgp"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
          "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{"Net", " ", "Force"}], "*)"}], "\[IndentingNewLine]", 
          RowBox[{"Fnet", "=", 
