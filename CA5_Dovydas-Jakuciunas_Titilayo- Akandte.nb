          "\[IndentingNewLine]", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{
           "Force", " ", "due", " ", "to", " ", "Gravity", " ", "Parallel", 
            " ", "to", " ", "Surface"}], "*)"}], "\[IndentingNewLine]", 
          RowBox[{"Fgp", "=", 
           RowBox[{"Fg", "-", "Fgn"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"FgpL", "=", 
           RowBox[{"Norm", "[", "Fgp", "]"}]}], ";", "\[IndentingNewLine]", 
          "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{"Normal", " ", "Force"}], "*)"}], "\[IndentingNewLine]", 
          RowBox[{"Fn", "=", 
           RowBox[{"-", "Fgn"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"FnL", "=", 
           RowBox[{"Norm", "[", "Fn", "]"}]}], ";", "\[IndentingNewLine]", 
          "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{"Determine", " ", "Friction", " ", "Type"}], "*)"}], 
          "\[IndentingNewLine]", 
          RowBox[{"FfU", "=", 
           RowBox[{"uStatic", " ", "FnL"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"frictionType", "=", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"FfU", "<=", "FgpL"}], ",", "\"\<Kinetic\>\"", ",", 
             "\"\<Static\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
          "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{"Frictional", " ", "Forces"}], "*)"}], 
          "\[IndentingNewLine]", 
          RowBox[{"FfHat", "=", 
